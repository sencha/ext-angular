{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "paths": {
    "@angular/*": [
      "./node_modules/@angular/*"
    ],
    "@sencha/*": [
      "./node_modules/@sencha/*"
    ],
  }
}
