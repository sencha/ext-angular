{
  "name": "ext-angular-boilerplate-angular-cli",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "buildprod": "ng build && npm run gzipext && npm run http",
    "gzip": "npm run gzipbuild && npm run gzipext",
    "gzipbuild": "npx gzip dist/*.js",
    "gzipext": "npx gzip dist/ext/*.js",
    "replace": "npx replace-in-file /.js/g .js.gz dist/index.html --isRegex",
    "http": "npx http-server dist -g -o",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
