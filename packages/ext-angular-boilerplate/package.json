{
  "name": "@sencha/ext-angular-boilerplate",
  "version": "7.2.0",
  "description": "ext-angular-boilerplate",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --env.browser=yes --env.verbose=no",
    "build": "npm run clean && npm run webpack1 && npm run gzip && npm run http",
    "clean": "rimraf build",
    "xwebpack": "npm run webpack1 && npm run webpack2",
    "webpack1": "cross-env webpack --env.environment=production --env.treeshake=yes --env.verbose=no",
    "xwebpack2": "cross-env webpack --env.environment=production --env.treeshake=no --env.basehref=/",
    "gzip": "npm run gzipbuild && npm run gzipext",
    "gzipbuild": "npx gzip build/*.js",
    "gzipext": "npx gzip build/ext/*.js",
    "replace": "npx replace-in-file /.js/g .js.gz build/index.html --isRegex",
    "http": "npx http-server build -g -o"
  },
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "@angular-builders/custom-webpack": "^9.0.0",
    "@angular-builders/dev-server": "^7.3.1",
    "@sencha/ext-angular-modern": "~7.2.0",
    "@sencha/ext-webpack-plugin": "~7.2.0",
    "@sencha/ext": "~7.2.0",
    "@sencha/ext-modern": "~7.2.0",
    "@sencha/ext-modern-treegrid": "~7.2.0",
    "@sencha/ext-modern-theme-material": "~7.2.0",
    "@sencha/ext-ux": "~7.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@ngtools/webpack": "~9.1.0",
    "core-js": "~3.6.4",
    "zone.js": "~0.10.3",
    "typescript": "3.7.5",
    "@types/core-js": "~2.5.3",
    "@types/node": "~13.9.5",
    "rimraf": "~3.0.2",
    "cross-env": "~7.0.2",
    "portfinder": "~1.0.25",
    "gzip-cli": "^1.1.0",
    "replace-in-file": "^5.0.2",
    "http-server": "^0.12.1",
    "css-loader": "~3.4.2",
    "file-loader": "~6.0.0",
    "html-loader": "~1.0.0",
    "style-loader": "~1.1.3",
    "to-string-loader": "~1.1.6",
    "html-webpack-plugin": "3.2.0",
    "base-href-webpack-plugin": "~2.0.0",
    "webpack-filter-warnings-plugin": "~1.2.1",
    "webpack": "~4.42.1",
    "webpack-cli": "~3.3.11",
    "webpack-dev-server": "~3.10.3"
  },
